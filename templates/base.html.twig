<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}{{ appName }} - {% endblock %}</title>
        <link rel="shorcut icon" type="image/png" href="{{ asset('img/logo.png') }}">
        {% block stylesheets %}
        {% endblock %}

        {% block javascripts %}
            {% block importmap %}{{ importmap('app') }}{% endblock %}
        {% endblock %}
    </head>
    <body>
        <main class="container-fluid min-vh-100 row">
            {% include 'nav.html.twig' %}
            <section class="col-auto p-5">
                {% block body %}{% endblock %}
            </section>
        </main>
    </body>
</html>
